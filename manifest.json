{
    "name": "BaclkgroundColor",
    "manifest_version": 2,
    "version": "1.0",
    "browser_action": {
        "default_title": "BaclkgroundColor"
    },
    "web_accessible_resources": [
        "components/panel/*.*"
    ],
    "background": {
        "persistent": true,
        "scripts": ["background.js"]
    },
    "icons": { "120": "static/icon.png" },
    "permissions": ["tabs"],
    "content_scripts": [{
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "css": [
            "app/content_scripts.css"
        ],
        "js": [
            "app/content_scripts.js",
            "app/Panel.js",
            "app/dict_src.js",
            "app/dict.js"
        ]
    }]
}