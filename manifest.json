{
    "name": "BaclkgroundColor",
    "manifest_version": 2,
    "version": "1.0",
    "browser_action": {
        "default_title": "BaclkgroundColor"
    },
    "web_accessible_resources": [
        "components/panel/*.*",
        "components/card/*.*",
        "app/*.*"
    ],
    "background": {
        "persistent": true,
        "scripts": ["background.js"]
    },
    "icons": { "120": "static/icon.png" },
    "permissions": [
        "tabs",
        "storage"
    ],
    "content_scripts": [{
        "matches": [
            "http://*/*",
            "https://*/*",
            "file://*/*"
        ],
        "css": [
            "app/content_scripts.css"
        ],
        "js": [
            "components/panel/Panel.js",
            "components/card/Card.js",
            "app/content_scripts.js",
            "app/dict_src.js",
            "app/dict.js"
        ]
    }]
}